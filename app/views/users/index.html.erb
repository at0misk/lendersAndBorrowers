<h1>Lender</h1>
<fieldset id="register">
	<legend>Register:</legend>
	<form action="users" method="post">
	<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
	<p>First Name: <input type="text" name="user[first_name]"></p>
	<p>Last Name: <input type="text" name="user[last_name]"></p>
	<p>Email: <input type="text" name="user[email]"></p>
	<p>Password: <input type="password" name="user[password]"></p>
	<p>Money: <input type="number" name="user[balance]"></p>
	<input type="hidden" name="user[account_type]" value="lender">
	<p><input type="submit" value="Register!"></p>
	</form>
	<a href="/login">Already a member?</a>
</fieldset>
<h1>Borrower</h1>
<fieldset id="register">
	<legend>Register:</legend>
	<form action="users" method="post">
	<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
	<p>First Name: <input type="text" name="user[first_name]"></p>
	<p>Last Name: <input type="text" name="user[last_name]"></p>
	<p>Email: <input type="text" name="user[email]"></p>
	<p>Password: <input type="password" name="user[password]"></p>
	<p>Need Money For: <input type="text" name="user[reason]"></p>
	<p>Description: <textarea name="user[description]"></textarea></p>
	<p>Ammount Needed: <input type="number" name="user[request]"></p>
	<input type="hidden" name="user[account_type]" value="borrower">
	<input type="hidden" name="user[balance]" value="0">
	<p><input type="submit" value="Register!"></p>
	</form>
	<a href="/login">Already a member?</a>
</fieldset>